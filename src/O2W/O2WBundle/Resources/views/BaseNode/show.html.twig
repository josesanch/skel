 2<h1>BaseNode</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ entity.title }}</td>
        </tr>
        <tr>
            <th>Path</th>
            <td>{{ entity.path }}</td>
        </tr>
        <tr>
            <th>Body</th>
            <td>{{ entity.body }}</td>
        </tr>
        <tr>
            <th>Blocks</th>
            <td>{{ entity.blocks }}</td>
        </tr>
        <tr>
            <th>Files</th>
            <td>{{ entity.files }}</td>
        </tr>
        <tr>
            <th>Slug</th>
            <td>{{ entity.slug }}</td>
        </tr>
        <tr>
            <th>Controller</th>
            <td>{{ entity.controller }}</td>
        </tr>
        <tr>
            <th>Language</th>
            <td>{{ entity.language }}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updatedat</th>
            <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Metadescription</th>
            <td>{{ entity.metaDescription }}</td>
        </tr>
        <tr>
            <th>Metakeywords</th>
            <td>{{ entity.metaKeywords }}</td>
        </tr>
        <tr>
            <th>Revision</th>
            <td>{{ entity.revision }}</td>
        </tr>
        <tr>
            <th>Tags</th>
            <td>{{ entity.tags }}</td>
        </tr>
        <tr>
            <th>Published</th>
            <td>{{ entity.published }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ entity.type }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('basenode') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('basenode_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('basenode_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
